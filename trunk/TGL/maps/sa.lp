<levelpack>
    <id>sa</id>
    <name>Second Assault</name>
    <description>Maps included with Super Transball 2: Second Assault</description>
    <creator>
        <name>Santiago Ontañón Villar</name>
        <nickname>Popolon</nickname>
    </creator>
    <levels>
        <level>
            <map>maps/secondassault/sa-level1.tgl</map>
            <name>Wellcome back!</name>
            <description>This level is just to warm up again...</description>
            <points>2</points>
        </level>
        <level>
            <map>maps/secondassault/sa-level2.tgl</map>
            <name>Deep in the earth</name>
            <description>Just a second warm up level...</description>
            <points>2</points>
        </level>
        <level>
            <map>maps/secondassault/sa-level3.tgl</map>
            <name>High accuracy cannons</name>
            <description>Look out for the red directional cannons.../They can surprise you!</description>
            <points>3</points>
        </level>
        <level>
            <map>maps/secondassault/sa-level4.tgl</map>
            <name>Underground base</name>
            <description>Under this innocent mountain lies a strongly/defended base. Take care, it's more difficult/than it looks!</description>
            <points>4</points>
        </level>
        <level>
            <map>maps/secondassault/sa-level5.tgl</map>
            <name>Impossible?</name>
            <description>Just use your imagination.../There's an easy way to complete this one...</description>
            <points>4</points>
        </level>
        <level>
            <map>maps/secondassault/sa-level6.tgl</map>
            <name>Tank upgrade</name>
            <description>Did you think that tanks were easy?/Just see what these new babies can do!</description>
            <points>4</points>
        </level>
        <level>
            <map>maps/secondassault/sa-level7.tgl</map>
            <name>Long way to return...</name>
            <description>This is a test to your nerves./Play slowly and carefully.</description>
            <points>5</points>
        </level>
        <level>
            <map>maps/secondassault/sa-level8.tgl</map>
            <name>Headquarters revisited</name>
            <description>Well, you made it again! But this time,/they are better prepared!</description>
            <initial-fuel>50</initial-fuel>
            <points>4</points>
        </level>
    </levels>
</levelpack>
