<levelpack>
    <id>expert</id>
    <name>Expert Pack</name>
    <description>Set of levels just for experts</description>
    <creator>
        <name>Santiago Ontañón Villar</name>
        <nickname>Popolon</nickname>
    </creator>
    <levels>
        <level>
            <map>maps/expert/expert-level1.tgl</map>
            <name>Directional cannons</name>
            <description>I hope you like dodging bullets!</description>
        </level>
        <level>
            <map>maps/expert/expert-level2.tgl</map>
            <name>Directional cannons 2</name>
            <description>If you are a true expert, the challenge/in this one is to finish it without firing/a single bullet!</description>
        </level>
        <level>
            <map>maps/expert/expert-level3.tgl</map>
            <name>Master dodger</name>
            <description>This level is difficult. Really difficult./But if you believe you are an expert it should/be a piece of cake!</description>
        </level>
        <level>
            <map>maps/expert/expert-level4.tgl</map>
            <name>No comments</name>
            <description>If you have made it till here, man!/You are a true expert... but things can/sill be more difficult...</description>
        </level>
    </levels>
</levelpack>
